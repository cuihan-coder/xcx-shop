<view class="content"><form><view class="input-block"><view class="input-title">预约项目名称</view><input class="input-class" placeholder-class="input-plaorder-class" type="text" placeholder="请输入预约项目名称" data-event-opts="{{[['input',[['__set_model',['','service_name','$event',[]]]]]]}}" value="{{service_name}}" bindinput="__e"/></view><view class="input-block"><view class="input-title">小孩出生年月</view><picker class="picker-class" name="birthday" mode="date" value="{{birthday}}" data-event-opts="{{[['change',[['changebirthday',['$event']]]]]}}" bindchange="__e"><view class="input-class">{{birthday}}</view></picker></view><view class="input-block"><view class="input-title">联系电话</view><input class="input-class" placeholder-class="input-plaorder-class" type="text" placeholder="请输入联系电话" data-event-opts="{{[['input',[['__set_model',['','tel_phone','$event',[]]]]]]}}" value="{{tel_phone}}" bindinput="__e"/></view><view class="input-block"><view class="input-title">联系人</view><input class="input-class" placeholder-class="input-plaorder-class" type="text" placeholder="请输入联系人姓名" data-event-opts="{{[['input',[['__set_model',['','name','$event',[]]]]]]}}" value="{{name}}" bindinput="__e"/></view><view class="input-block"><view class="input-title">到店日期</view><picker class="picker-class" name="arrival_time" mode="date" value="{{arrival_time}}" data-event-opts="{{[['change',[['changedays',['$event']]]]]}}" bindchange="__e"><view class="input-class">{{arrival_time}}</view></picker></view><view class="input-block"><view class="input-title">到店时间</view><picker class="picker-class" name="hours" mode="time" value="{{hours}}" start="00:00" end="24:00" data-event-opts="{{[['change',[['changeHour',['$event']]]]]}}" bindchange="__e"><view class="input-class">{{hours}}</view></picker></view><view class="textarea-block"><view class="textarea-title">备注</view><textarea class="textarea-class" placeholder-class="textarea-plaorder-class" placeholder="请输入备注" data-event-opts="{{[['input',[['__set_model',['','remark','$event',[]]]]]]}}" value="{{remark}}" bindinput="__e"></textarea></view><button data-event-opts="{{[['tap',[['subInfo']]]]}}" class="yyue-btn" bindtap="__e">提交预约申请</button></form></view>