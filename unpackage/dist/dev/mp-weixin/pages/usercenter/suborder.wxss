	height: 170rpx;
}
.scroll-item-image image{
	width: 100%;
	height: 100%;
}
.scroll-item-price{
	height: 36rpx;
	font-size: 36rpx;
	color: #f34e4a;
	text-align: center;
}
.scroll-item-price text{
	font-size: 30rpx;
	padding-right: 6rpx;
}
.scroll-item-markprice{
	padding: 0;
	height: 32rpx;
	font-size: 30rpx;
	color: #999;
	text-align: center;
}
.index-title{
	width: 100%;
	margin: 20rpx auto 10rpx;
	height: 50rpx;
}
.index-title text:nth-child(1){
	display: inline-block;
	font-size: 34rpx;
	color: #333;
}
.index-title text:nth-child(2){
	display: inline-block;
	font-size: 28rpx;
	color: #999;
	padding-left: 10rpx;
	margin-left: 10rpx;
	border-left: solid 1rpx #999;
}
.scroll-view-x{
	width: 100%;
	margin-top: 10rpx;
	white-space: nowrap;
	margin-bottom: 10rpx;
}
.scroll-view-x text{
	display: inline-block;
	line-height: 50rpx;
	color: #666;
	font-size: 32rpx;
	padding: 0 20rpx;
}
.scroll-active{
	border-bottom: solid 1rpx #F6C2C5;
}
.goods-list{
	width: 96%;
	padding: 10rpx 2%;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-flex-wrap: wrap;
	        flex-wrap: wrap;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
}
.goods-list-item{
	width: 45%;
	padding: 2%;
	margin-bottom: 20rpx;
	background: #fff;
	-webkit-border-radius: 12rpx;
	        border-radius: 12rpx;
}
.goods-item-image{
	width: 100%;
	height: 250rpx;
}
.goods-item-image image{
	width: 100%;
	height: 100%;
}
.goods-item-content{
	width: 100%;
	padding: 0 0;
	font-size: 26rpx;
	color: #333;
}
.goods-item-tag{
	width: 100%;
	padding: 5rpx 0;
	font-size: 22rpx;
}
.goods-item-tag-mj{
	display: inline-block;
	-webkit-border-radius: 32rpx;
	        border-radius: 32rpx;
	background: #FFEBEE;
	color: #FE604F;
	padding: 3rpx 10rpx;
	margin-right: 10rpx;
}
.goods-item-tag-wyg{
	display: inline-block;
	-webkit-border-radius: 32rpx;
	        border-radius: 32rpx;
	background: #E4F6F7;
	color: #5BB9E6;
	padding: 3rpx 10rpx;
	margin-right: 10rpx;
}
.goods-item-price {
	color: #FE716B;
}
.goods-item-price text:nth-child(1){
	font-size: 22rpx;
	padding-right: 4rpx;
}
.goods-item-price text:nth-child(2){
	font-size: 32rpx;
}
.not-data{
	width: 100%;
	display: grid;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
}
.not-data image{
	width: 128rpx;
	height: 128rpx;
}
.not-data view{
	text-align: center;
	color: #999;
	font-size: 20rpx;
}
page{
	background: #f5f5f5;
}

